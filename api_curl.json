{
    "deepl": {
        "translate": {
            "url": "curl -X POST 'https://api-free.deepl.com/v2/translate' --header 'Authorization: DeepL-Auth-Key %API_KEY%' --data-urlencode 'text=%QUERY%' --data-urlencode 'target_lang=%LANG%'",
            "args": ["API_KEY", "QUERY", "LANG"]
        }
    },
    "palm": {
        "textgen": {
            "url": "curl -H 'Content-Type: application/json' -d '{ \"prompt\": { \"text\": \"%QUERY%\"} }'  \"https://generativelanguage.googleapis.com/v1beta2/models/text-bison-001:generateText?key=%API_KEY%\"",
            "args": ["API_KEY", "QUERY"]
        }
    },
    "": {}
}